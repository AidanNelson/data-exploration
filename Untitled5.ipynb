      "        return $('<div id=\"' + highlight_id + '\" class=\"hit-highlight\" />'\r\n",
      "        ).css({\r\n",
      "            'left': left + '%',\r\n",
      "            'top': top + '%',\r\n",
      "            'width': width + '%',\r\n",
      "            'height': height + '%',\r\n",
      "            'position': 'absolute',\r\n",
      "            'z-index': 10\r\n",
      "        });\r\n",
      "    }\r\n",
      "\r\n",
      "    function position_overlays_slide(page_id, page_width, page_height, coords_list) {\r\n",
      "        var $slide_li = $(\"#segment-li-\" + page_id);\r\n",
      "        if (!($slide_li.length)) {\r\n",
      "            // no actual search result for this page -- exit this function\r\n",
      "            return true;\r\n",
      "        }\r\n",
      "        if (coords_list.length == 0) {\r\n",
      "            // coords data doesn't exist -- exit this function\r\n",
      "            return true;\r\n",
      "        }\r\n",
      "        var $slide_image = $slide_li.find('.rsImg').first();\r\n",
      "        var slide_li_width = $slide_li.width();\r\n",
      "        var slide_li_height = $slide_li.height();\r\n",
      "        var slide_image_marginLeft = parseFloat($slide_image.css('marginLeft'));\r\n",
      "        var slide_image_marginTop = parseFloat($slide_image.css('marginTop'));\r\n",
      "        var slide_image_width = $slide_image.width();\r\n",
      "        var slide_image_height = $slide_image.height();\r\n",
      "\r\n",
      "        $.each(coords_list, function (index, coords) {\r\n",
      "            // word coordinates: [left, top, width, height]\r\n",
      "            var highlight_id = \"highlight_\" + page_id + '_' + index;\r\n",
      "            $(\"#\" + highlight_id).remove(); //will destroy the old overlay, if it exists\r\n",
      "\r\n",
      "            // The overlays are positioned relative to their parent, the\r\n",
      "            // $slide_li, but they need to show up in the right place relative\r\n",
      "            // to the $slide_image.\r\n",
      "            // The $slide_li is quite a bit larger than the $slide_image,\r\n",
      "            // so we take the ratio of these two sizes and use it to scale the\r\n",
      "            // positions, widths, and heights of the overlays.\r\n",
      "            // We also make the overlays a little larger so they're more visible\r\n",
      "            // and we shift them a little.\r\n",
      "            var marginLeft_ratio = slide_image_marginLeft / slide_li_width;\r\n",
      "            var marginTop_ratio = slide_image_marginTop / slide_li_height;\r\n",
      "            var width_ratio = slide_image_width / slide_li_width;\r\n",
      "            var height_ratio = slide_image_height / slide_li_height;\r\n",
      "            var overlay_left = 100 * (marginLeft_ratio + coords[0] / page_width * width_ratio) - 0.3;\r\n",
      "            var overlay_top = 100 * (marginTop_ratio + coords[1] / page_height * height_ratio) - 0.3;\r\n",
      "            var overlay_width = 100 * coords[2] / page_width * width_ratio + 0.7;\r\n",
      "            var overlay_height = 100 * coords[3] / page_height * height_ratio + 0.7;\r\n",
      "\r\n",
      "            var $highlight_overlay = create_overlay(highlight_id,\r\n",
      "                overlay_left, overlay_top, overlay_width, overlay_height);\r\n",
      "\r\n",
      "            $slide_li.append($highlight_overlay);\r\n",
      "        });\r\n",
      "    }\r\n",
      "\r\n",
      "    function position_overlays_icon(page_id, page_width, page_height, coords_list) {\r\n",
      "        var $icon_div = $(\"#segment-div-\" + page_id);\r\n",
      "        var $icon = $(\"#segment-image-\" + page_id);\r\n",
      "        if (!($icon_div.length)) {\r\n",
      "            // no actual search result for this page -- exit this function\r\n",
      "            return true;\r\n",
      "        }\r\n",
      "        if (coords_list.length == 0) {\r\n",
      "            // coords data doesn't exist -- exit this function\r\n",
      "            return true;\r\n",
      "        }\r\n",
      "        var icon_div_width = $icon_div.width();\r\n",
      "        var icon_div_height = $icon_div.height();\r\n",
      "        var icon_image_width = $icon.width();\r\n",
      "        var icon_image_height = $icon.height();\r\n",
      "\r\n",
      "        $.each(coords_list, function (index, coords) {\r\n",
      "            // word coordinates: [left, top, width, height]\r\n",
      "            var highlight_id = \"highlight_\" + page_id + '_' + index;\r\n",
      "            $(\"#\" + highlight_id).remove();\r\n",
      "\r\n",
      "            // The overlays are positioned relative to their parent, the\r\n",
      "            // $icon_div, but they need to show up in the right place relative\r\n",
      "            // to the $icon image.\r\n",
      "            // The $icon_div is apparently slightly larger than the $icon image,\r\n",
      "            // so we take the ratio of these two sizes and use it to scale the\r\n",
      "            // positions, widths, and heights of the overlays.\r\n",
      "            // We also make the overlays a little larger so they're more visible\r\n",
      "            // and we shift them a little.\r\n",
      "            var width_ratio = icon_image_width / icon_div_width;\r\n",
      "            var height_ratio = icon_image_height / icon_div_height;\r\n",
      "            var overlay_left = 100 * coords[0] / page_width * width_ratio - 1.2;\r\n",
      "            var overlay_top = 100 * coords[1] / page_height * height_ratio - 0.5;\r\n",
      "            var overlay_width = 100 * coords[2] / page_width * width_ratio + 2.9;\r\n",
      "            var overlay_height = 100 * coords[3] / page_height * height_ratio + 1.3;\r\n",
      "\r\n",
      "            var $highlight_overlay = create_overlay(highlight_id,\r\n",
      "                overlay_left, overlay_top, overlay_width, overlay_height);\r\n",
      "\r\n",
      "            $icon_div.append($highlight_overlay);\r\n",
      "        });\r\n",
      "    }\r\n",
      "\r\n",
      "    var view_style;\r\n",
      "    \r\n",
      "    view_style = \"iconic\";\r\n",
      "    \r\n",
      "\r\n",
      "    function make_request_page(position_overlays_fn) {\r\n",
      "        return function(page_id) {\r\n",
      "            var url = word_coordinates_queries[page_id];\r\n",
      "            if (!url) {\r\n",
      "                \r\n",
      "                return true;\r\n",
      "            }\r\n",
      "            $.ajax({\r\n",
      "                url: url,\r\n",
      "                type: 'get',\r\n",
      "                timeout: 30000,\r\n",
      "                dataType: 'json',\r\n",
      "                success: function(page_results_object) {\r\n",
      "                    var page_details = page_results_object[page_id] || page_results_object[page_id + \".xml\"];\r\n",
      "                    if (!page_details) {\r\n",
      "                        //No results for this page\r\n",
      "                        return true;\r\n",
      "                    }\r\n",
      "                    var page = {\r\n",
      "                        id: page_id,\r\n",
      "                        width: null,\r\n",
      "                        height: null,\r\n",
      "                        coords_list: []\r\n",
      "                    };\r\n",
      "                    page['width'] = parseFloat(page_details.width);\r\n",
      "                    page['height'] = parseFloat(page_details.height);\r\n",
      "                    $.each(page_details['searchTerms'], function(term, term_coords_list) {\r\n",
      "                        page['coords_list'] = page['coords_list'].concat(term_coords_list);\r\n",
      "                    });\r\n",
      "\r\n",
      "                    \r\n",
      "                    \r\n",
      "\r\n",
      "                    word_coordinates_pages[page_id] = page;\r\n",
      "                    position_overlays_fn(page_id, page['width'], page['height'], page['coords_list']);\r\n",
      "                }\r\n",
      "            });\r\n",
      "        };\r\n",
      "    }\r\n",
      "\r\n",
      "    var word_coordinates_queries;\r\n",
      "    var word_coordinates_pages;\r\n",
      "\r\n",
      "    \r\n",
      "    word_coordinates_queries = {};\r\n",
      "    \r\n",
      "\r\n",
      "    \r\n",
      "    word_coordinates_pages = {};\r\n",
      "    \r\n",
      "\r\n",
      "    if (view_style == \"slideshow\") {\r\n",
      "        slider = create_slideshow($('.search-results.slideshow-view'));\r\n",
      "\r\n",
      "        if ($.isEmptyObject(word_coordinates_queries)) {\r\n",
      "            // Queries do not exist.  End execution early after setting up the\r\n",
      "            // slider.\r\n",
      "            return true;\r\n",
      "        }\r\n",
      "        // else - query exists, hook into royalSlider rsAfterContentSet event\r\n",
      "        //       to load highlights as images load lazily.\r\n",
      "        var request_page_for_slide = make_request_page(position_overlays_slide);\r\n",
      "        slider.ev.on('rsAfterContentSet', function(event, d) {\r\n",
      "            var page_id = d.content.attr('name');\r\n",
      "            request_page_for_slide(page_id);\r\n",
      "        });\r\n",
      "\r\n",
      "        \r\n",
      "        $(window).resize(function() {\r\n",
      "            setTimeout(function() {\r\n",
      "                $.each(word_coordinates_pages, function(page_id, page) {\r\n",
      "                    position_overlays_slide(page_id, page['width'], page['height'], page['coords_list']);\r\n",
      "                });\r\n",
      "            }, 300);\r\n",
      "        });\r\n",
      "    }\r\n",
      "    else { // view style: iconic\r\n",
      "        \r\n",
      "        \r\n",
      "\r\n",
      "        \r\n",
      "        \r\n",
      "        \r\n",
      "        if ($.isEmptyObject(word_coordinates_queries)) {\r\n",
      "            // Queries do not exist.  No highlights.  End execution.\r\n",
      "            return true;\r\n",
      "        } else {\r\n",
      "            var request_page_for_icon = make_request_page(position_overlays_icon);\r\n",
      "            var $iconic = $('.iconic');\r\n",
      "            $iconic.one('load', function(event) {\r\n",
      "                var page_id = event.target.name;\r\n",
      "                if (page_id) {\r\n",
      "                    request_page_for_icon(page_id);\r\n",
      "                }\r\n",
      "            }).each(function() {\r\n",
      "                // to make sure the \"on load\" event occurs even if the\r\n",
      "                // images are already cached:\r\n",
      "                if (this.complete) {\r\n",
      "                    $(this).load();\r\n",
      "                }\r\n",
      "            });\r\n",
      "        }\r\n",
      "    }\r\n",
      "});\r\n",
      "</script>\n",
      "<script src=\"https://cdn.loc.gov/js/global/metrics/sc/s_code.js?35836.31053\"></script>\n",
      "<!-- omniture: do not remove this comment, it breaks omniture if it isnt present -->\n",
      "</body>\n",
      "</html>\n",
      ">\n"
     ]
    }
